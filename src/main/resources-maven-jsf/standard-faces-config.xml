        <converter-class>org.primefaces.convert.IntegerConverter</converter-class>
    </converter>

    <converter>
        <converter-for-class>java.lang.Integer</converter-for-class>
        <converter-class>org.primefaces.convert.IntegerConverter</converter-class>
    </converter>

    <converter>
        <converter-id>javax.faces.Double</converter-id>
